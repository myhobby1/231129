#include <stdio.h>

int main(void)
{
    int a;
    int b,c;
    double da;
  
    a=10;
    b=a;
    c=a+20;
    da=3.5;
    ch='A';
  
  printf("변수a의값:%d\n",a);
  printf("변수b의값:%d\n",b);
  printf("변수c의값:%d\n",c);
  printf("변수da의값:%.1lf\n",da);
  printf("변수ch의값:%c\n",ch);

  return 0;
}
